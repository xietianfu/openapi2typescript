    "/mydata/search": {
      "get": {
        "parameters": [
          {
            "description": "The search keyword to look for in MyData names or tags.",
            "in": "query",
            "name": "query",
            "required": true,
            "type": "string"
          },
          {
            "description": "Type for the keyword. 'folder' or 'file'",
            "in": "query",
            "name": "type",
            "required": true,
            "type": "string"
          }
        ],
        "responses": {
          "200": {
            "description": "An array of MyData entries that match the search criteria.",
            "items": {
              "id": {
                "example": 1,
                "format": "int64",
                "type": "integer"
              },
              "name": {
                "example": "Sample Data",
                "type": "string"
              },
              "properties": null,
              "tags": {
                "example": "example tag",
                "items": null,
                "type": "string"
              },
              "type": "object"
            },
            "schema": null,
            "type": "array"
          }
        },
        "summary": "Search one Mydata for its name or tags."
      }
    },
